generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model User {
  id               Int     @id @default(autoincrement())
  email            String  @unique
  username         String  @unique
  password         String
  rol              String
  securityQuestion String
  securityAnswer   String
  pic              String?
  state            Boolean @default(true)
}
